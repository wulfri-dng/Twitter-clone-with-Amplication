datasource mongo {
  provider = "mongodb"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  birthday    String
  email       String
  id          String @id @default(auto()) @map("_id") @mongo.ObjectId
  likedTweets Json?
  password    String
  roles       Json
  tweets      Json?
  username    String @unique
}
